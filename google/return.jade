html
	head
		title facebook callback
	body
		script(src="/s/dist/scripts/post-message-polyfill.min.js")
		script
			var params = location.search.substring(1).split("&"), l,i, split, code;
			for(i=0,l=params.length;i<l;i++) {
				split = params[i].split("=");
				if(split[0]=="code") {
					code = split[1];
					break;
				}
			}
			var auth = {
				command:"signin",
				auth: {
					google: {
						code: code
					}
				}
			};
			window.opener.postMessage(auth,"*");
			window.close();
