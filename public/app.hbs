<!DOCTYPE html>
<html manifest="/manifest.appcache" lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0">
	<meta name="mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-capable" content="yes">

	<meta name="description" content="{{seo.head.description}}">

	<meta name="twitter:card" content="summary" />
	<meta name="twitter:title" content="{{seo.head.title}}">
	<meta name="twitter:description" content="{{seo.head.description}}">
	<meta name="twitter:image" content="{{seo.head.picture}}">

	<meta property="og:type" content="website" />
	<meta property="og:site_name" content={{seo.name}} />
	<meta property="og:title" content="{{seo.head.title}}">
	<meta property="og:description" content="{{seo.head.description}}">
	<meta property="og:image" content="{{seo.head.picture}}">
	<meta property="og:url" content="{{seo.path}}">

	<title>{{seo.head.title}}</title>

	<link rel="image_src" href="{{seo.head.picture}}">

	<link rel="shortcut icon" href="/s/assets/heyneighbor-favicon.png" type="image/x-icon">

	<link rel="stylesheet" type="text/css" media="screen" href="/s/dist/styles/style.min.css">

	{{#if analytics.id}}
		<script>
			var _gaq = _gaq || [];
			_gaq.push(['_setAccount', '{{analytics.id}}']);
			_gaq.push(['_trackPageview']);

			(function() {
				var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
				ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
				var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
			})();
		</script>
	{{/if}}

	{{#if errorception.id}}
		<script>
			(function(_,e,rr,s){_errs=[s];var c=_.onerror;_.onerror=function(){var a=arguments;_errs.push(a);
			c&&c.apply(this,a)};var b=function(){var c=e.createElement(rr),b=e.getElementsByTagName(rr)[0];
			c.src="//beacon.errorception.com/"+s+".js";c.async=!0;b.parentNode.insertBefore(c,b)};
			_.addEventListener?_.addEventListener("load",b,!1):_.attachEvent("onload",b)})
			(window,document,"script","{{errorception.id}}");
		</script>
	{{/if}}

	{{#if weiner.enabled}}
		<script src="{{weiner.protocol}}//{{weiner.host}}/target/target-script-min.js#{{weiner.appid}}"></script>
	{{/if}}
</head>

<body>
	<!-- [if lt IE 9]>
		<div class="ie-warning">
			<div class="ie-warning-content">
				<h1>Your browser isnâ€™t supported :-(</h1>
				<p>Please <a href="http://browsehappy.com">upgrade</a> to a modern browser.</p>
			</div>
		</div>
	<![endif]-->

	<!-- SEO -->
	<main itemscope style="position: absolute; top: -9999em; left: -9999em;">
		{{{seo.body}}}
	</main>

	<div id="app-client" class="app-client"></div>

	<div class="splash-screen" id="splash-screen">
		<div class="splash-screen-inner">
			 <div class="chat-bubble-container">
				  <div class="chat-bubble">
					   <div class="chat-bubble-line"></div>
					   <div class="chat-bubble-line"></div>
					   <div class="chat-bubble-line"></div>
				  </div>
				  <div class="chat-bubble-shadow"></div>
			 </div>
		</div>
	</div>

	<script src="/s/dist/scripts/app.bundle.min.js"></script>

	<script async defer src="//login.persona.org/include.js"></script>

</body>
</html>
